<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.range.venus.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_contractFragment"
            app:destination="@id/contractFragment" />
        <action
            android:id="@+id/action_homeFragment_to_tableFragment"
            app:destination="@id/tableFragment" />
        <action
            android:id="@+id/action_homeFragment_to_attenFragment"
            app:destination="@id/attenFragment" />
    </fragment>
    <fragment
        android:id="@+id/contractFragment"
        android:name="com.range.venus.ui.fragment.ContractFragment"
        android:label="fragment_contract"
        tools:layout="@layout/fragment_contract" />
    <fragment
        android:id="@+id/tableFragment"
        android:name="com.range.venus.ui.fragment.TableFragment"
        android:label="TableFragment"
        tools:layout="@layout/fragment_table" />
    <fragment
        android:id="@+id/attenFragment"
        android:name="com.range.venus.ui.fragment.AttenFragment"
        android:label="fragment_atten"
        tools:layout="@layout/fragment_atten" />
</navigation>